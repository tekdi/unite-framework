    {
        "widName" : "Latest Courses",
        "page_id" : "2",
        "source" : "sunbird",
        "service" : "sbcourses",
        "defaultConfig" : {
            "baseUrl": "https://staging.open-sunbird.org",
            "search_filter": {
                        "request":{
                            "filters":{
                                "contentType":["Course"],
                                "objectType":["Content"],
                                "status":["Live"]
                            },
                        "sort_by":{
                            "lastPublishedOn":"desc"
                        },
                        "limit":10
                    }
                }
        },
        "renderer" : "carousel",
        "mapper" :{
            "image_url": "appIcon", 
            "caption": "name",
            "description": "description",
            "link" : {
                "page_id" : "6",
                "urlParams" : {
                    "courseId" : "IL_UNIQUE_ID"
                }
            }
        }
    },
